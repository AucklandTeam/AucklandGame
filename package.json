{
  "name": "aucklandgame",
  "version": "1.0.0",
  "description": "Asteroid game on js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prod:build": "NODE_ENV=production NODE_PATH=. npm run clean && webpack --mode production",
    "prod:server": "nodemon index.js --watch dist/server.js",
    "start": "NODE_ENV=production npm-run-all --print-label --parallel prod:*",
    "dev:webpack": "webpack --mode=development --watch",
    "dev:server": "nodemon index.js --watch dist/server.js --trace-warnings",
    "dev": "NODE_ENV=development npm run clean && npm-run-all --print-label --parallel dev:*",
    "test": "jest --env=jsdom",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,html,css,scss}\"",
    "lint": "run-p lint:**",
    "lint:script": "eslint --fix --cache --quiet --ext .ts,.tsx ./",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss}\"",
    "lint:typescript": "tsc --project tsconfig.json --noEmit",
    "husky-install": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AucklandTeam/AucklandGame.git"
  },
  "keywords": [],
  "author": "Auckland Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AucklandTeam/AucklandGame/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/mediaMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^server/(.*)$": "<rootDir>/src/server/$1",
      "^client/(.*)$": "<rootDir>/src/client/$1",
      "^components/(.*)$": "<rootDir>/src/client/components/$1",
      "^pages/(.*)$": "<rootDir>/src/client/pages/$1",
      "^shared/(.*)$": "<rootDir>/src/client/components/@shared/$1",
      "^styles/(.*)$": "<rootDir>/src/client/styles/$1",
      "^static/(.*)$": "<rootDir>/static/$1"
    }
  },
  "homepage": "https://github.com/AucklandTeam/AucklandGame",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/compression": "^1.7.2",
    "@types/jest": "^27.4.1",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.6.2",
    "@types/serialize-javascript": "^5.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@webpack-cli/generators": "^3.0.1",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^6.7.1",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon-webpack-plugin": "^4.7.1",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-modules-values": "^4.0.0",
    "prettier": "2.6.1",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "ts-loader": "^9.2.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@hot-loader/react-dom": "^17.0.2",
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.17.3",
    "history": "^5.3.0",
    "i18next": "^21.8.10",
    "i18next-browser-languagedetector": "^6.1.4",
    "identity-obj-proxy": "^3.0.0",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-i18next": "^11.17.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-test-renderer": "^17.0.0",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.20.1",
    "sequelize-typescript": "^2.1.3",
    "serialize-javascript": "^6.0.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0"
  },
  "preinstall": "([ ! -f package-lock.json ] && npm install --package-lock-only --ignore-scripts --no-audit); npx npm-force-resolutions",
  "resolutions": {
    "got": "11.8.5",
    "ejs": "3.1.7",
    "glob-parent": "5.1.2"
  }
}
