{
  "name": "aucklandgame",
  "version": "1.0.0",
  "description": "Asteroid game on js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "NODE_ENV=development ts-node src/server",
    "spa": "NODE_ENV=development webpack serve --config webpack/configs/client.conf.ts",
    "start": "NODE_ENV=production run-s server:*",
    "server:build": "tsc && webpack",
    "server:start": "node --trace-warnings --experimental-modules --es-module-specifier-resolution=node server.js",
    "spa:serve": "webpack serve --mode development --open --hot --config webpack/configs/client.conf.ts",
    "spa:build": "webpack --mode=production --node-env=production",
    "test": "jest --env=jsdom",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,html,css,scss}\"",
    "lint": "run-p lint:**",
    "lint:script": "eslint --fix --cache --quiet --ext .ts,.tsx ./",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madcher/game_sprint5-6.git"
  },
  "keywords": [],
  "author": "Auckland Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/madcher/game_sprint5-6/issues"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "homepage": "https://github.com/madcher/game_sprint5-6#readme",
